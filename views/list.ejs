<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>List Student</title>
    <style>

    </style>
</head>
<body>
    <div class="container">
        <a href="./add">Add New</a>
        <% danhsach.forEach(function(student1){ %>
            <div class="tableListStudent">
                <img src="./upload/<%- student1.Image %>" alt="" width="100" height="100">
                <div class="studentName">
                    <%- student1.Name %>
                </div>
                <div class="ageName">
                    <%- student1.Age %>
                </div>
                <div class="className">
                    <%- student1.Class %>
                </div>
                <div class=dateName">
                    <%- student1.Date %>
                </div>
                <div class="buttonList">
                    <a href="./edit/<%- student1._id %>">Edit</a>
                    <a onclick="return confirm('Are you sure, you want to delete <%- student1.Name %>?')" href="./delete/<%- student1._id %>">Delete</a>
                </div>
            </div>
        <% }); %>
    </div>
</body>
</html>